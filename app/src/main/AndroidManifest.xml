<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestApplication"
        android:usesCleartextTraffic="true">
        <!-- HTTPでも通信できるようにする:usesCleartextTraffic -->
        <activity
            android:name=".LogicalHostActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".LogicalSwitchActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".LogicalRouterActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".CableActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".RoutingActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ACLActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <!--通信の許可-->
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>